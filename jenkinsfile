pipeline {
  agent any 
    stages {
      stage('build docker image') {
        steps {
	  sh 'docker images'
	  sh 'git branch: 'main', url:'https://github.com/ravi2703/DOCKER.git'
	}
	stage('docker') {
	  steps {
	    sh 'cd Docker'
	    sh 'docker build -t .'
	    sh 'dcoker ps'
	  }
	}
     }
   }
 }
